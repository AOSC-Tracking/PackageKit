#: ../client/pk-console.c:338
msgid "Package"
msgstr "Pakiet"

#: ../client/pk-console.c:340
msgid "Updates"
msgstr "Aktualizuje"

#: ../client/pk-console.c:342
msgid "Obsoletes"
